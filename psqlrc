-- dont make noises when loading psqlrc
\set ECHO none

-- Use vim for editing functions
\set EDITOR /usr/bin/vim

-- Unicode Handlers
\encoding unicode
\pset linestyle unicode

-- Disable pager
\pset pager off

-- Change formatting of 'null'
\pset null Ã¸

-- enable timing report
\timing

-- Prompt
\set PROMPT1 '(%n@%M:%>) [%/] > '
\set PROMPT2 ''
